###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/age
:age rdf:type owl:DatatypeProperty ;
     rdfs:domain :Character ,
                 <https://www.wikidata.org/wiki/Q95074> ;
     rdfs:range xsd:int ;
     rdfs:comment "The number of years a character has existed for."@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/class
:class rdf:type owl:DatatypeProperty ;
       rdfs:domain :Character ,
                   <https://www.wikidata.org/wiki/Q95074> ;
       rdfs:range [ rdf:type rdfs:Datatype ;
                    owl:oneOf [ rdf:type rdf:List ;
                                rdf:first "Demigod" ;
                                rdf:rest [ rdf:type rdf:List ;
                                           rdf:first "Dragon" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "Dwarf" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "Elf" ;
                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                            rdf:first "Fairy" ;
                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                       rdf:first "God" ;
                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                  rdf:first "Human" ;
                                                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                                                             rdf:first "Mutant" ;
                                                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                                                        rdf:first "Ogre" ;
                                                                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                                                                   rdf:first "Robot" ;
                                                                                                                                   rdf:rest rdf:nil
                                                                                                                                 ]
                                                                                                                      ]
                                                                                                           ]
                                                                                                ]
                                                                                     ]
                                                                          ]
                                                               ]
                                                    ]
                                         ]
                              ]
                  ] ;
       rdfs:comment "The \"race/species\" that a character belongs to."@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain owl:Thing ;
      rdfs:range rdfs:Literal ;
      rdfs:comment "The main way something is referred to"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/sex
:sex rdf:type owl:DatatypeProperty ;
     rdfs:domain :Character ,
                 <https://www.wikidata.org/wiki/Q95074> ;
     rdfs:range xsd:string ;
     rdfs:comment "The gender of a character."@en .


