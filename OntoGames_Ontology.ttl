                                                                                   owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> ;
                                                                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                                 ] .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Asset
:Asset rdf:type owl:Class ;
       rdfs:comment "An important component of a videogame: could be visual or auditory"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Car
:Car rdf:type owl:Class ;
     rdfs:subClassOf :Asset ;
     rdfs:comment "A specific game asset that is used in racing-like games that represents a car"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Card
:Card rdf:type owl:Class ;
      rdfs:subClassOf :Asset ;
      rdfs:comment "A game asset that can be used in the context of a card game"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Character
:Character rdf:type owl:Class ;
           rdfs:subClassOf <https://www.wikidata.org/wiki/Q95074> ;
           rdfs:comment "A character of a game"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Competitive_Game
:Competitive_Game rdf:type owl:Class ;
                  rdfs:subClassOf <http://dbpedia.org/ontology/Software> ,
                                  <http://schema.org/Product> ,
                                  <http://www.wikidata.org/entity/Game> ;
                  rdfs:comment "A videogame in which players compete to reach a goal."@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/DLC
:DLC rdf:type owl:Class ;
     rdfs:subClassOf <http://dbpedia.org/ontology/Software> ,
                     <http://schema.org/Product> ,
                     <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
     rdfs:comment "Downloadable Content for a videogame. It generally is an expansion to the base version of the Game"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Female_Protagonist_Game
:Female_Protagonist_Game rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( :Story_Game
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :hasProtagonist ;
                                                                      owl:someValuesFrom [ owl:intersectionOf ( :Character
                                                                                                                [ rdf:type owl:Restriction ;
                                                                                                                  owl:onProperty :hasSex ;
                                                                                                                  owl:hasValue :Female
                                                                                                                ]
                                                                                                              ) ;
                                                                                           rdf:type owl:Class
                                                                                         ]
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf :Story_Game ,
                                         [ owl:intersectionOf ( :Story_Game
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasProtagonist ;
                                                                  owl:someValuesFrom [ owl:intersectionOf ( :Character
                                                                                                            [ rdf:type owl:Restriction ;
                                                                                                              owl:onProperty :hasSex ;
                                                                                                              owl:hasValue :Female
                                                                                                            ]
                                                                                                          ) ;
                                                                                       rdf:type owl:Class
                                                                                     ]
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                         rdfs:comment "A story game in which the protagonist is of gender: Female"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Gender
:Gender rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:oneOf ( :Female
                                          :Male
                                          :NotSpecified
                                        )
                            ] ;
        rdfs:subClassOf [ rdf:type owl:Class ;
                          owl:oneOf ( :Female
                                      :Male
                                      :NotSpecified
                                    )
                        ] ;
        rdfs:comment "The gender of some characters: in the domain of videogames could be something other than male/female" .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Killer
:Killer rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Character
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :kills ;
                                                     owl:someValuesFrom :Character
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Character ,
                        [ owl:intersectionOf ( :Character
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :kills ;
                                                 owl:someValuesFrom :Character
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] ;
        rdfs:comment "A character that has killed some other character"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Lover
:Lover rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Character
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :loves ;
                                                    owl:someValuesFrom :Character
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf :Character ,
                       [ owl:intersectionOf ( :Character
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :loves ;
                                                owl:someValuesFrom :Character
                                              ]
                                            ) ;
                         rdf:type owl:Class
                       ] ;
       rdfs:comment "A character that is in love with some other character"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Map
:Map rdf:type owl:Class ;
     rdfs:comment "A fictionary representation of either a real or an imaginary place in which the action is set during a game"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Protagonist
:Protagonist rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Character
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :isProtagonistOf ;
                                                          owl:someValuesFrom <http://www.wikidata.org/entity/Game>
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Character ,
                             [ owl:intersectionOf ( :Character
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :isProtagonistOf ;
                                                      owl:someValuesFrom <http://www.wikidata.org/entity/Game>
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
             rdfs:comment """A character that is considered the leader / main character of a game.
Usually the  one the player plays as."""@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Saga
:Saga rdf:type owl:Class ;
      rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ,
                      <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ,
                      <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
      rdfs:comment """A list of games that have something in common and have an intrinsic order.
They might share the universe, the topic, the producer, ..."""@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Story_Game
:Story_Game rdf:type owl:Class ;
            rdfs:subClassOf <http://dbpedia.org/ontology/Software> ,
                            <http://schema.org/Product> ,
                            <http://www.wikidata.org/entity/Game> ;
            rdfs:comment "A game in which one or more players are told a story"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Studio
:Studio rdf:type owl:Class ;
        rdfs:comment "A set of people / an institution / a company that makes one or more videogames"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Villain
:Villain rdf:type owl:Class ;
         rdfs:subClassOf :Character ,
                         <https://www.wikidata.org/wiki/Q95074> ;
         rdfs:comment "The main antagonist of the protagonist." .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Weapon
:Weapon rdf:type owl:Class ;
        rdfs:subClassOf :Asset ;
        rdfs:comment "A game asset that can be used to shoot projectiles"@en .


###  http://www.wikidata.org/entity/Game
<http://www.wikidata.org/entity/Game> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://dbpedia.org/ontology/Software> ,
                                                      <http://schema.org/Product> ,
                                                      <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                                      rdfs:comment "Some sort of videogame"@en ;
                                      owl:equivalentClass "https://www.wikidata.org/wiki/Q7889" .


###  https://www.wikidata.org/wiki/Q95074
<https://www.wikidata.org/wiki/Q95074> rdf:type owl:Class ;
                                       rdfs:comment "Represents the context of videogames characters, which are immaginary or not-physical" .


