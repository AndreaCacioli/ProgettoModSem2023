@prefix : <http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix bag: <https://bag2.basisregistraties.overheid.nl/bag/def/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@base <http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/> .

<http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology> rdf:type owl:Ontology ;
                                                                         owl:imports <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ;
                                                                         <http://purl.org/dc/elements/1.1/creator> "Andrea Cacioli" ,
                                                                                                                   "Samuele Cavagnino" ;
                                                                         <http://purl.org/dc/elements/1.1/date> "2024-01-09T11:43:00"^^xsd:dateTime ;
                                                                         <http://purl.org/dc/elements/1.1/description> "An ontology that wants to describe videogames's dual nature of product and story scenario"@en ;
                                                                         <http://purl.org/dc/elements/1.1/title> "OntoGames"@en ;
                                                                         vann:preferredNamespacePrefix "http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology" ;
                                                                         vann:preferredNamespaceUri "OntoGames" ;
                                                                         rdfs:comment "Ontology focused on videogames' characters interactions"@en ;
                                                                         rdfs:label "OntoGames"@en ;
                                                                         owl:versionInfo 1.0 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#equivalentClass
owl:equivalentClass rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> owl:propertyChainAxiom ( <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                                                                        <http://www.ontologydesignpatterns.org/cp/owl/list.owl#nextItem>
                                                                                      ) ,
                                                                                      ( <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                                                                        <http://www.ontologydesignpatterns.org/cp/owl/list.owl#previousItem>
                                                                                      ) .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/DLCOf
:DLCOf rdf:type owl:ObjectProperty ;
       owl:inverseOf :hasDLC ;
       rdfs:domain :DLC ;
       rdfs:range <http://www.wikidata.org/entity/Game> ;
       rdfs:comment "relation from a Game to a DLC"@en ;
       rdfs:label "DLCOf"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/appearsIn
:appearsIn rdf:type owl:ObjectProperty ;
           owl:inverseOf :hasCharacter ;
           rdfs:domain :Character ;
           rdfs:range <http://www.wikidata.org/entity/Game> ;
           rdfs:comment "relation from a character to a game"@en ;
           rdfs:label "appearsIn"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/belongsTo
:belongsTo rdf:type owl:ObjectProperty ;
           owl:inverseOf :hasAsset ;
           rdfs:domain :Asset ;
           rdfs:range :Character ;
           rdfs:comment "relation from a asset to a character"@en ;
           rdfs:label "belongsTo"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/hasAsset
:hasAsset rdf:type owl:ObjectProperty ;
          rdfs:domain :Character ;
          rdfs:range :Asset ;
          rdfs:comment "relation from a character to an asset"@en ;
          rdfs:label "hasAsset"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/hasCharacter
:hasCharacter rdf:type owl:ObjectProperty ;
              rdfs:domain <http://www.wikidata.org/entity/Game> ;
              rdfs:range :Character ;
              rdfs:comment "relation from a game to a character"@en ;
              rdfs:label "hasCharacter"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/hasDLC
:hasDLC rdf:type owl:ObjectProperty ;
        rdfs:domain <http://www.wikidata.org/entity/Game> ;
        rdfs:range :DLC ;
        rdfs:comment "relation from a game to a dlc"@en ;
        rdfs:label "hasDLC"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/hasMap
:hasMap rdf:type owl:ObjectProperty ;
        owl:inverseOf :isMapOf ;
        rdfs:domain <http://www.wikidata.org/entity/Game> ;
        rdfs:range :Map ;
        rdfs:comment "relation from a game to a map"@en ;
        rdfs:label "hasMap"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/hasProtagonist
:hasProtagonist rdf:type owl:ObjectProperty ;
                owl:inverseOf :isProtagonistOf ;
                rdfs:domain <http://www.wikidata.org/entity/Game> ;
                rdfs:range :Character ;
                rdfs:comment "relation from a game to a character"@en ;
                rdfs:label "hasProtagonist"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/hasRelative
:hasRelative rdf:type owl:ObjectProperty ,
                      owl:SymmetricProperty ;
             rdfs:domain :Character ;
             rdfs:range :Character ;
             rdfs:comment "relation from a character to a character"@en ;
             rdfs:label "hasRelative"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/hasSex
:hasSex rdf:type owl:ObjectProperty ;
        rdfs:domain :Character ;
        rdfs:range :Gender ;
        rdfs:comment "relation from a character to a gender"@en ;
        rdfs:label "hasSex"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/isAssetOf
:isAssetOf rdf:type owl:ObjectProperty ;
           rdfs:domain :Asset ;
           rdfs:range <http://www.wikidata.org/entity/Game> ;
           owl:propertyChainAxiom ( :belongsTo
                                    :appearsIn
                                  ) ;
           rdfs:comment "relation from a asset to a game"@en ;
           rdfs:label "isAssetOf"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/isLovedBy
:isLovedBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :loves ;
           rdfs:domain :Character ;
           rdfs:range :Lover ;
           rdfs:comment "relation from a character to a lover"@en ;
           rdfs:label "isLovedBy"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/isMapOf
:isMapOf rdf:type owl:ObjectProperty ;
         rdfs:domain :Map ;
         rdfs:range <http://www.wikidata.org/entity/Game> ;
         rdfs:comment "relation from a map to a game"@en ;
         rdfs:label "isMapOf"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/isProtagonistOf
:isProtagonistOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :appearsIn ;
                 rdfs:domain :Character ;
                 rdfs:range <http://www.wikidata.org/entity/Game> ;
                 rdfs:comment "relation from a character to a game"@en ;
                 rdfs:label "isProtagonistOf"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/isUsedBy
:isUsedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :usesWeapon ;
          rdfs:domain :Weapon ;
          rdfs:range :Character ;
          rdfs:comment "relation from a weapon to a character"@en ;
          rdfs:label "isUsedBy"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/killedBy
:killedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :kills ;
          rdfs:domain :Character ;
          rdfs:range :Killer ;
          rdfs:comment "relation from a character to a killer"@en ;
          rdfs:label "killedBy"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/kills
:kills rdf:type owl:ObjectProperty ;
       rdfs:domain :Killer ;
       rdfs:range :Character ;
       rdfs:comment "relation from a killer to a character"@en ;
       rdfs:label "kills"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/loves
:loves rdf:type owl:ObjectProperty ;
       rdfs:domain :Lover ;
       rdfs:range :Character ;
       rdfs:comment "relation from a lover to a character"@en ;
       rdfs:label "loves"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/madeBy
:madeBy rdf:type owl:ObjectProperty ;
        owl:inverseOf :makes ;
        rdfs:domain <http://dbpedia.org/ontology/Software> ;
        rdfs:range :Studio ;
        rdfs:comment "relation from a software to a studio"@en ;
        rdfs:label "madeBy"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/makes
:makes rdf:type owl:ObjectProperty ;
       rdfs:domain :Studio ;
       rdfs:range <http://dbpedia.org/ontology/Software> ;
       rdfs:comment "relation from a studio to a software"@en ;
       rdfs:label "makes"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/mutuallyLoves
:mutuallyLoves rdf:type owl:ObjectProperty ,
                        owl:SymmetricProperty ;
               rdfs:domain :Character ;
               rdfs:range :Character ;
               rdfs:comment "relation from a character to a character"@en ;
               rdfs:label "mutuallyLoves"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/represents
:represents rdf:type owl:ObjectProperty ;
            rdfs:domain :Card ;
            rdfs:range :Character ;
            rdfs:comment "relation from a card to a character"@en ;
            rdfs:label "represents"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/sagaMadeBy
:sagaMadeBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :studioMakesSaga ;
            rdfs:domain :Saga ;
            rdfs:range :Studio ;
            owl:propertyChainAxiom ( <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                     :madeBy
                                   ) ;
            rdfs:comment "relation from a saga to a studio"@en ;
            rdfs:label "sagaMadeBy"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/studioMakesSaga
:studioMakesSaga rdf:type owl:ObjectProperty ;
                 rdfs:domain :Studio ;
                 rdfs:range :Saga ;
                 rdfs:comment "relation from a studio to a saga"@en ;
                 rdfs:label "studioMakesSaga"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/usesWeapon
:usesWeapon rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Character ;
            rdfs:range :Weapon ;
            rdfs:comment "relation from a character to a weapon"@en ;
            rdfs:label "usesWeapon"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/age
:age rdf:type owl:DatatypeProperty ;
     rdfs:domain :Character ;
     rdfs:range xsd:int ;
     rdfs:comment "The number of years a character has existed for."@en ;
     rdfs:label "age"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/class
:class rdf:type owl:DatatypeProperty ;
       rdfs:domain :Character ;
       rdfs:range [ rdf:type rdfs:Datatype ;
                    owl:oneOf [ rdf:type rdf:List ;
                                rdf:first "Demigod" ;
                                rdf:rest [ rdf:type rdf:List ;
                                           rdf:first "Dragon" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "Dwarf" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "Elf" ;
                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                            rdf:first "Fairy" ;
                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                       rdf:first "Fatui" ;
                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                  rdf:first "God" ;
                                                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                                                             rdf:first "Human" ;
                                                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                                                        rdf:first "Mutant" ;
                                                                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                                                                   rdf:first "Ogre" ;
                                                                                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                                                                                              rdf:first "Robot" ;
                                                                                                                                              rdf:rest rdf:nil
                                                                                                                                            ]
                                                                                                                                 ]
                                                                                                                      ]
                                                                                                           ]
                                                                                                ]
                                                                                     ]
                                                                          ]
                                                               ]
                                                    ]
                                         ]
                              ]
                  ] ;
       rdfs:comment "The \"race/species\" that a character belongs to."@en ;
       rdfs:label "class"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain owl:Thing ;
      rdfs:range rdfs:Literal ;
      rdfs:comment "The main way something is referred to"@en ;
      rdfs:label "name"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/sex
:sex rdf:type owl:DatatypeProperty ;
     rdfs:domain :Character ;
     rdfs:range xsd:string ;
     rdfs:comment "The gender of a character."@en ;
     rdfs:label "sex"@en .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Software
<http://dbpedia.org/ontology/Software> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://schema.org/Product> ;
                                       rdfs:comment "Superclass of VideoGame"@en .


###  http://schema.org/Product
<http://schema.org/Product> rdf:type owl:Class ;
                            rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                            rdfs:comment "Represents something that can be sold"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#List
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> rdf:type owl:Class ;
                                                             rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> rdf:type owl:Class ;
                                                                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Adult
:Adult rdf:type owl:Class ;
       rdfs:subClassOf :Character ;
       owl:disjointWith :Minor ;
       rdfs:comment "Character which age is over 18"@en ;
       rdfs:label "Adult"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Asset
:Asset rdf:type owl:Class ;
       rdfs:comment "An important component of a videogame: could be visual or auditory"@en ;
       rdfs:label "Asset"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Car
:Car rdf:type owl:Class ;
     rdfs:subClassOf :Asset ;
     rdfs:comment "A specific game asset that is used in racing-like games that represents a car"@en ;
     rdfs:label "Car"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Card
:Card rdf:type owl:Class ;
      rdfs:subClassOf :Asset ;
      rdfs:comment "A game asset that can be used in the context of a card game"@en ;
      rdfs:label "Card"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Card_Game
:Card_Game rdf:type owl:Class ;
           rdfs:subClassOf <http://www.wikidata.org/entity/Game> ;
           rdfs:comment "Represents a game played with cards"@en ;
           rdfs:label "Card_Game"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Character
:Character rdf:type owl:Class ;
           rdfs:subClassOf <https://www.wikidata.org/wiki/Q95074> ;
           rdfs:comment "A character of a game"@en ;
           rdfs:label "Character"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Competitive_Game
:Competitive_Game rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.wikidata.org/entity/Game> ;
                  rdfs:comment "A videogame in which players compete to reach a goal."@en ;
                  rdfs:label "Competitive_Game"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/DLC
:DLC rdf:type owl:Class ;
     rdfs:subClassOf <http://dbpedia.org/ontology/Software> ;
     rdfs:comment "Downloadable Content for a videogame. It generally is an expansion to the base version of the Game"@en ;
     rdfs:label "DLC"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Female_Protagonist_Game
:Female_Protagonist_Game rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( :Story_Game
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :hasProtagonist ;
                                                                      owl:someValuesFrom [ owl:intersectionOf ( :Character
                                                                                                                [ rdf:type owl:Restriction ;
                                                                                                                  owl:onProperty :hasSex ;
                                                                                                                  owl:hasValue :Female
                                                                                                                ]
                                                                                                              ) ;
                                                                                           rdf:type owl:Class
                                                                                         ]
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf :Story_Game ;
                         rdfs:comment "A story game in which the protagonist is of gender: Female"@en ;
                         rdfs:label "Female_Protagonist_Game"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Gender
:Gender rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:oneOf ( :Female
                                          :Male
                                          :NotSpecified
                                        )
                            ] ;
        rdfs:comment "The gender of some characters: in the domain of videogames could be something other than male/female"@en ;
        rdfs:label "Gender"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Killer
:Killer rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Character
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :kills ;
                                                     owl:someValuesFrom :Character
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Character ;
        rdfs:comment "A character that has killed some other character"@en ;
        rdfs:label "Killer"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Lover
:Lover rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Character
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :loves ;
                                                    owl:someValuesFrom :Character
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf :Character ;
       rdfs:comment "A character that is in love with some other character"@en ;
       rdfs:label "Lover"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Map
:Map rdf:type owl:Class ;
     rdfs:comment "A fictionary representation of either a real or an imaginary place in which the action is set during a game"@en ;
     rdfs:label "Map"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Minor
:Minor rdf:type owl:Class ;
       rdfs:subClassOf :Character ;
       rdfs:comment "Character which age is under 18"@en ;
       rdfs:label "Minor"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Protagonist
:Protagonist rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Character
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :isProtagonistOf ;
                                                          owl:someValuesFrom <http://www.wikidata.org/entity/Game>
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Character ;
             rdfs:comment """A character that is considered the leader / main character of a game.
Usually the  one the player plays as."""@en ;
             rdfs:label "Protagonist"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/RPG
:RPG rdf:type owl:Class ;
     rdfs:subClassOf :Story_Game ;
     rdfs:comment "Represents a role played game"@en ;
     rdfs:label "RPG"@en .


###  http://www.semanticweb.org/cava/ontologies/2023/11/OntoGames_Ontology/Saga
:Saga rdf:type owl:Class ;
      rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
      rdfs:comment """A list of games that have something in common and have an intrinsic order.
They might share the universe, the topic, the producer, ..."""@en ;
      rdfs:label "Saga"@en .


